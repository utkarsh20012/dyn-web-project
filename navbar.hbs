<nav class="navbar navbar-expand-lg navbar-dark bg-dark py-3">
  <div class="container">
    <a class="navbar-brand" href="#">
      <img src="{{data.brandIconUrl}}" width="30" height="30" class="d-inline-block align-top" alt="" style="border-radius: 50%;">
      <span class="navbar-brand-text">{{data.brandName}}</span>
    </a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
        {{#each data.links}}
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="{{this.url}}">
            <span class="nav-link-text">{{this.label}}</span>
          </a>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</nav>
